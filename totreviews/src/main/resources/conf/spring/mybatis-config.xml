<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD MyBatis Configuration 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    
    <properties resource="props/db.properties" />
    
    <settings>
        <setting name="jdbcTypeForNull" value="VARCHAR"/>
    </settings>  
    
    <typeAliases>
    	<typeAlias alias="TripVO" type="totreviews.domain.TripVO"/>
    	<typeAlias alias="TReviewVO" type="totreviews.domain.TReviewVO"/>
    	<typeAlias alias="TReviewReqDTO" type="totreviews.domain.TReviewReqDTO"/>
    	<typeAlias alias="TReviewResDTO" type="totreviews.domain.TReviewResDTO"/>
    	<typeAlias alias="TReviewImageVO" type="totreviews.domain.TReviewImageVO"/>
    	<typeAlias alias="CommentVO" type="totreviews.domain.CommentVO"/>
    	<typeAlias alias="CommentReqDTO" type="totreviews.domain.CommentReqDTO"/>
    	<typeAlias alias="CourseDTO" type="totreviews.domain.CourseDTO"/>
    	<typeAlias alias="CourseResDTO" type="totreviews.domain.CourseResDTO"/>
    	<typeAlias alias="TourVO" type="totreviews.domain.TourVO"/>
    	<typeAlias alias="PageDTO" type="totreviews.common.page.PageDTO"/>
    	<typeAlias alias="PageReqDTO" type="totreviews.common.page.PageReqDTO"/>
    	<typeAlias alias="PageResDTO" type="totreviews.common.page.PageResDTO"/>
    	<typeAlias alias="ReportVO" type="totreviews.domain.ReportVO"/>
    	<typeAlias alias="ReportCommentDTO" type="totreviews.domain.ReportCommentDTO"/>
    	<typeAlias alias="HistoryVO" type="totreviews.domain.HistoryVO"/>
    </typeAliases>
    
    <mappers>
        <mapper resource="conf/mapper/TReviewMapper.xml"/>
        <mapper resource="conf/mapper/CourseMapper.xml"/>
        <mapper resource="conf/mapper/TourMapper.xml"/>
        <mapper resource="conf/mapper/TripMapper.xml"/>
        <mapper resource="conf/mapper/CommentMapper.xml"/>
        <mapper resource="conf/mapper/HistoryMapper.xml"/>
        
        <mapper resource="conf/mapper/AdminTReviewMapper.xml"/>
        <mapper resource="conf/mapper/AdminCommentMapper.xml"/>
    </mappers>
    
</configuration>
